#ifdef __OBJC__
    #import <Foundation/Foundation.h>
#endif

#define Require(cond) if (!(cond)) {\
    @throw [NSException exceptionWithName:NSInvalidArgumentException\
                                   reason:[NSString stringWithFormat:@"Invalid argument. Condition '%s' was not met", #cond]\
                                 userInfo:nil];\
}
